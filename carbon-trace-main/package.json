{
  "name": "carbon-trace",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate": "openapi-typescript ./src/assets/api/AssetAdministrationShellRegistrySwagger.json -o ./src/lib/api/AssetAdministrationShellRegistry.d.ts && openapi-typescript ./src/assets/api/AssetAdministrationShellRepositorySwagger.json -o ./src/lib/api/AssetAdministrationShellRepository.d.ts && openapi-typescript ./src/assets/api/SubmodelRepositorySwagger.json -o ./src/lib/api/SubmodelRepository.d.ts && openapi-typescript ./src/assets/api/AssetAdditionalsSwagger.json -o ./src/lib/api/AssetAdditionals.d.ts"
  },
  "dependencies": {
    "@heroicons/vue": "^2.1.3",
    "@observablehq/plot": "^0.6.16",
    "@tailwindcss/typography": "^0.5.15",
    "@vueuse/core": "^10.10.0",
    "colorjs.io": "^0.5.2",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "daisyui": "^4.12.10",
    "echarts": "^5.5.1",
    "htl": "^0.3.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "lz-string": "^1.5.0",
    "openapi-fetch": "^0.9.7",
    "pako": "^2.1.0",
    "tailwindcss": "^3.4.4",
    "uuid": "^10.0.0",
    "vue": "^3.4.21",
    "vue-echarts": "^6.7.3",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/d3": "^7.4.3",
    "@types/d3-sankey": "^0.12.4",
    "@types/lodash": "^4.17.6",
    "@types/luxon": "^3.4.2",
    "@types/lz-string": "^1.5.0",
    "@types/node": "^20.12.5",
    "@types/pako": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "@vue/eslint-config-airbnb-with-typescript": "^8.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.1.2",
    "openapi-typescript": "^6.7.6",
    "prettier": "^3.2.5",
    "sass": "^1.77.4",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  }
}
